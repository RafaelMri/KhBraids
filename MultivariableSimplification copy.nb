 RowBox[{
  RowBox[{"gaussianElimination", "[", "complex_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "zeroes", ",", "quotient", ",", "remainder", ",", "result1", ",", 
      "result2", ",", 
      RowBox[{"result", "=", "$Failed"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"zeroes", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Count", "[", 
         RowBox[{
          RowBox[{"#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}],
           ",", "0", ",", 
          RowBox[{"{", "2", "}"}]}], "]"}], "&"}], "/@", "complex"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
